<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ASolute_Mobile.MainMenu">
   
        <StackLayout>
        <ListView x:Name="MainMenuList" ItemTapped="selectMenu" IsPullToRefreshEnabled="True" Refreshing="refreshMainMenu"  SeparatorColor="White"></ListView>
        <ActivityIndicator x:Name="activityIndicator" IsRunning="False" VerticalOptions="Center" HorizontalOptions="Center" IsVisible="False"/>
        </StackLayout>
   
</ContentPage>